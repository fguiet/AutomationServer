docker run \
        -v /etc/localtime:/etc/localtime:ro \
        -v /etc/timezone:/etc/timezone:ro \
        --name influxdb \
        --network automationserver-network \
        -h docker-influxdb \
        -v /home/fred/applications/influxdb:/influxdb \
        -d \
        --restart=always \
        -p 8086:8086 \
        guiet/influxdb:1.5.1

